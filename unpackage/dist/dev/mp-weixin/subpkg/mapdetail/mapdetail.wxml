<block wx:if="{{address.title}}"><view class="general"><view class="outbox"><view class="bkgBox"><view class="base-info">{{address.title}}</view><swiper autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="{{true}}"><block wx:for="{{address.img}}" wx:for-item="item" wx:for-index="i" wx:key="i"><swiper-item><view class="swiper-item"><image src="{{item}}" mode="aspectFill"></image></view></swiper-item></block></swiper><view class="add-info"><uni-icons vue-id="bc62ee20-1" type="location-filled" size="24" bind:__l="__l"></uni-icons><view>{{"地址："+address.address}}</view></view><view class="add-info"><uni-icons vue-id="bc62ee20-2" type="info-filled" size="24" bind:__l="__l"></uni-icons><view>{{"门票："+address.ticket}}</view></view><view class="add-info" style="margin-bottom:10px;"><uni-icons vue-id="bc62ee20-3" type="flag-filled" size="24" bind:__l="__l"></uni-icons><view>{{"开放时间："+address.ontime}}</view></view><scroll-view class="intro" scroll-y="{{true}}"><view class="text">{{''+address.intro+''}}</view></scroll-view><view><video style="width:100%;margin:1px;" src="{{address.video}}" controls="{{true}}"></video></view></view></view></view></block>